          <xs:documentation xml:lang="sv">När tåget har ankommit eller avgått</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="TimeAtLocationWithSeconds" type="xs:dateTime">
