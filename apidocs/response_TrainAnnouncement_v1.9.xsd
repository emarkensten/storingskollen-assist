        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ActivityType" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="sv">"Ankomst" eller "Avgang"</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Advertised" type="xs:boolean" minOccurs="0">
