        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ScheduledDepartureDateTime" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation xml:lang="sv">Tågets annonserade avgångsdatum</xs:documentation>
        </xs:annotation>
      </xs:element>
